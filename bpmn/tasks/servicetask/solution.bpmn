      <bpmn:outgoing>Flow_1nwvtks</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:sequenceFlow id="Flow_1nwvtks" sourceRef="StartEvent_1" targetRef="Activity_0tfti2v" />
    <bpmn:userTask id="Activity_0tfti2v" name="Decide about request" camunda:assignee="${driver}">
      <bpmn:extensionElements>
        <camunda:formData>
          <camunda:formField id="name" label="Request from" type="string">
            <camunda:validation>
              <camunda:constraint name="readonly" />
            </camunda:validation>
          </camunda:formField>
          <camunda:formField id="destination" label="Destination" type="string">
            <camunda:validation>
              <camunda:constraint name="readonly" />
            </camunda:validation>
          </camunda:formField>
          <camunda:formField id="answer" label="Answer" type="string">
            <camunda:validation>
              <camunda:constraint name="required" />
            </camunda:validation>
          </camunda:formField>
        </camunda:formData>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_1nwvtks</bpmn:incoming>
      <bpmn:outgoing>Flow_0ug2o8y</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="Flow_0ug2o8y" sourceRef="Activity_0tfti2v" targetRef="Task_SendMail" />
    <bpmn:endEvent id="Event_0wrvsd6" name="Request answered">
      <bpmn:incoming>Flow_1mif4z4</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_1mif4z4" sourceRef="Task_SendMail" targetRef="Event_0wrvsd6" />
    <bpmn:serviceTask id="Task_SendMail" name="Send Mail" camunda:modelerTemplate="com.camunda.university.MailTask" camunda:delegateExpression="${mailDelegate}">
      <bpmn:extensionElements>
        <camunda:inputOutput>
          <camunda:inputParameter name="subject">Status update</camunda:inputParameter>
          <camunda:inputParameter name="content">${answer}</camunda:inputParameter>
          <camunda:inputParameter name="recipient">${email}</camunda:inputParameter>
        </camunda:inputOutput>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_0ug2o8y</bpmn:incoming>
      <bpmn:outgoing>Flow_1mif4z4</bpmn:outgoing>
